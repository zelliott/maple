{
  "manifest_version": 2,
  "name": "MAPLE",
  "description": "",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.ncbi.nlm.nih.gov/pubmed/*"],
      "css": ["content.css"],
      "js": ["jquery.js", "content.js"]
    }
  ]
}